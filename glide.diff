< func isHex(c byte) bool {
< 	switch {
< 	case c >= 'a' && c <= 'f':
< 		return true
< 	case c >= 'A' && c <= 'F':
< 		return true
< 	case c >= '0' && c <= '9':
< 		return true
< 	}
< 	return false
< }
< 
< // borrowed from net/url/url.go
< func unhex(c byte) byte {
< 	switch {
< 	case '0' <= c && c <= '9':
< 		return c - '0'
< 	case 'a' <= c && c <= 'f':
< 		return c - 'a' + 10
< 	case 'A' <= c && c <= 'F':
< 		return c - 'A' + 10
< 	}
< 	return 0
< }
