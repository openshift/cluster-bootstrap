< type item struct {
< 	t   itemType
< 	val string
< }
< 
< func (i item) String() string {
< 	switch i.t {
< 	case itemEOF:
< 		return "EOF"
< 	case itemError:
< 		return i.val
< 	}
< 	if len(i.val) > 10 {
< 		return fmt.Sprintf("%.10q...", i.val)
< 	}
< 	return fmt.Sprintf("%q", i.val)
< }
< 
< type itemType int
< 
< const (
< 	itemError itemType = iota
< 	itemEOF
< 
< 	itemDataPrefix
< 
< 	itemMediaType
< 	itemMediaSep
< 	itemMediaSubType
< 	itemParamSemicolon
< 	itemParamAttr
< 	itemParamEqual
< 	itemLeftStringQuote
< 	itemRightStringQuote
< 	itemParamVal
< 
< 	itemBase64Enc
< 
< 	itemDataComma
< 	itemData
