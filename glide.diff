< 	var doneFields []bool
< 	if d.strict {
< 		doneFields = make([]bool, len(sinfo.FieldsList))
< 	}
< 	for i := 0; i < l; i += 2 {
< 		ni := n.children[i]
< 		if isMerge(ni) {
< 			d.merge(n.children[i+1], out)
< 			continue
< 		}
< 		if !d.unmarshal(ni, name) {
< 			continue
< 		}
