< func unmarshal(in []byte, out interface{}, strict bool) (err error) {
< 	defer handleErr(&err)
< 	d := newDecoder(strict)
< 	p := newParser(in)
< 	defer p.destroy()
< 	node := p.parse()
< 	if node != nil {
< 		v := reflect.ValueOf(out)
< 		if v.Kind() == reflect.Ptr && !v.IsNil() {
< 			v = v.Elem()
< 		}
< 		d.unmarshal(node, v)
< 	}
< 	if len(d.terrors) > 0 {
< 		return &TypeError{d.terrors}
< 	}
< 	return nil
< }
