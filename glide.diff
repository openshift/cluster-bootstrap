< func (d *decodeState) literalNode() Node {
< 	start := d.off - 1
< 	// Can just use the interface version since this has no children
< 	node := Node{
< 		Start: start,
< 		Value: d.literalInterface(),
< 	}
< 	node.End = d.off - 1
< 	return node
< }
