< func fromYamlNode(n yaml.Node) tree.Node {
< 	m := tree.Marker{
< 		StartP: &tree.Pos{
< 			Line:   int64(n.Line),
< 			Column: int64(n.Column),
< 		},
< 	}
< 	switch n.Kind {
< 	case 0:
< 		// empty
< 		return nil
< 	case yaml.DocumentNode:
< 		if len(n.Content) == 0 {
< 			return nil
< 		}
