< 					return false
< 				}
< 				if !yaml_emitter_write_tag_handle(emitter, tag_directive.handle) {
< 					return false
< 				}
< 				if !yaml_emitter_write_tag_content(emitter, tag_directive.prefix, true) {
< 					return false
< 				}
< 				if !yaml_emitter_write_indent(emitter) {
< 					return false
< 				}
< 			}
< 		}
