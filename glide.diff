< 	case yaml.MappingNode:
< 		ret := tree.MapNode{
< 			Marker:   m,
< 			Children: make(map[string]tree.Node, len(n.Content)/2),
< 			Keys:     make(map[string]tree.Leaf, len(n.Content)/2),
< 		}
< 		// MappingNodes list keys and values like [k, v, k, v...]
< 		for i := 0; i < len(n.Content); i += 2 {
< 			key := *n.Content[i]
< 			value := *n.Content[i+1]
