< 	case '"': // string
< 		s, ok := unquote(item)
< 		if !ok {
< 			d.error(errPhase)
< 		}
< 		return s
< 
< 	default: // number
< 		if c != '-' && (c < '0' || c > '9') {
< 			d.error(errPhase)
< 		}
< 		n, err := d.convertNumber(string(item))
< 		if err != nil {
< 			d.saveError(err)
< 		}
< 		return n
< 	}
< }
