< 		// Read opening " of string key or closing }.
< 		d.scanWhile(scanSkipSpace)
< 		if d.opcode == scanEndObject {
< 			// closing } - can only happen on first iteration.
< 			break
< 		}
< 		if d.opcode != scanBeginLiteral {
< 			panic(phasePanicMsg)
