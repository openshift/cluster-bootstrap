< 		// Next token must be , or ].
< 		if d.opcode == scanSkipSpace {
< 			d.scanWhile(scanSkipSpace)
< 		}
< 		if d.opcode == scanEndArray {
< 			break
< 		}
< 		if d.opcode != scanArrayValue {
< 			panic(phasePanicMsg)
< 		}
< 	}
< 	return v
