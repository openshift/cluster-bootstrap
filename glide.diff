< 	for {
< 		r, size, err := reader.ReadRune()
< 		if err == io.EOF {
< 			break
< 		}
< 		if err != nil {
< 			return nil, err
< 		}
< 		if size > 1 {
< 			return nil, fmt.Errorf("rfc2396: non-ASCII char detected")
< 		}
