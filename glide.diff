< 		out[i], err = strconv.ParseFloat(d, 64)
< 		if err != nil {
< 			return err
< 		}
< 
< 	}
< 	if !s.changed {
< 		*s.value = out
< 	} else {
< 		*s.value = append(*s.value, out...)
< 	}
