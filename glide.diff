< 	tm, r = translate.Prefixed(tr, "verification", &from.Verification, &to.Verification)
< 	translate.MergeP(tr, tm, &r, "httpHeaders", &from.HTTPHeaders, &to.HTTPHeaders)
< 	to.Source = from.Source
