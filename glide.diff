< 			if err == io.EOF {
< 				return nil, fmt.Errorf("rfc2396: unexpected end of unescape sequence")
< 			}
< 			if err != nil {
< 				return nil, err
< 			}
< 			if !isHex(eb1) {
< 				return nil, fmt.Errorf("rfc2396: invalid char 0x%x in unescape sequence", r)
< 			}
< 			eb0, err := reader.ReadByte()
< 			if err == io.EOF {
< 				return nil, fmt.Errorf("rfc2396: unexpected end of unescape sequence")
< 			}
< 			if err != nil {
< 				return nil, err
< 			}
< 			if !isHex(eb0) {
