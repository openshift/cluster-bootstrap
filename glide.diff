< 	value := reflect.ValueOf(object)
< 	kind := value.Kind()
< 	isNilableKind := containsKind(
< 		[]reflect.Kind{
< 			reflect.Chan, reflect.Func,
< 			reflect.Interface, reflect.Map,
< 			reflect.Ptr, reflect.Slice},
< 		kind)
