< 			if !yaml_emitter_write_indicator(emitter, []byte("..."), true, false, false) {
< 				return false
< 			}
< 			if !yaml_emitter_write_indent(emitter) {
< 				return false
< 			}
< 		}
< 		if !yaml_emitter_flush(emitter) {
< 			return false
< 		}
< 		emitter.state = yaml_EMIT_END_STATE
< 		return true
< 	}
