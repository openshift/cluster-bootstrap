< 				return false
< 			}
< 		case 'k', 'K':
< 			if tr != kelvin {
< 				return false
< 			}
< 		default:
< 			return false
< 		}
< 		t = t[size:]
< 
< 	}
< 	if len(t) > 0 {
< 		return false
< 	}
< 	return true
< }
