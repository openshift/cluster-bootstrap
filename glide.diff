< func (s *uintSliceValue) Set(val string) error {
< 	ss := strings.Split(val, ",")
< 	out := make([]uint, len(ss))
< 	for i, d := range ss {
< 		u, err := strconv.ParseUint(d, 10, 0)
< 		if err != nil {
< 			return err
< 		}
< 		out[i] = uint(u)
< 	}
< 	if !s.changed {
< 		*s.value = out
< 	} else {
< 		*s.value = append(*s.value, out...)
< 	}
