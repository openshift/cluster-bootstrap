< 			return false
< 		}
< 		*i++
< 	} else {
< 		if !write(emitter, s, i) {
< 			return false
< 		}
< 		if emitter.column == 0 {
< 			emitter.space_above = true
< 		}
< 		emitter.column = 0
< 		emitter.line++
< 		// [Go] Do this here and above and drop from everywhere else (see commented lines).
< 		emitter.indention = true
< 	}
