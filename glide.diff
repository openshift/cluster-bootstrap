< 			path.New("yaml"),
< 		},
< 		// local specified
< 		{
< 			Resource{
< 				Local:       util.StrToPtr("hello"),
< 				Compression: util.StrToPtr("gzip"),
< 				Verification: Verification{
< 					Hash: util.StrToPtr("this isn't validated"),
< 				},
< 			},
< 			nil,
< 			path.New("yaml"),
< 		},
< 		// source + inline, invalid
< 		{
