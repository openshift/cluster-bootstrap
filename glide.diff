< func (t *nodeTracker) Exists(path string) bool {
< 	for _, m := range []map[string]int{t.fileMap, t.dirMap, t.linkMap} {
< 		if _, ok := m[path]; ok {
< 			return true
< 		}
< 	}
< 	return false
< }
