< //
< //     type T struct {
< //         F int `yaml:"a,omitempty"`
< //         B int
< //     }
< //     yaml.Marshal(&T{B: 2}) // Returns "b: 2\n"
< //     yaml.Marshal(&T{F: 1}} // Returns "a: 1\nb: 0\n"
< //
< func Marshal(in interface{}) (out []byte, err error) {
< 	defer handleErr(&err)
< 	e := newEncoder()
< 	defer e.destroy()
< 	e.marshalDoc("", reflect.ValueOf(in))
< 	e.finish()
< 	out = e.out
< 	return
< }
