< 		if !ok {
< 			d.error(errPhase)
< 		}
< 
< 		// Read : before value.
< 		if op == scanSkipSpace {
< 			op = d.scanWhile(scanSkipSpace)
< 		}
< 		if op != scanObjectKey {
< 			d.error(errPhase)
< 		}
< 
< 		// Read value.
< 		m[key] = d.valueInterface()
< 
< 		// Next token must be , or }.
< 		op = d.scanWhile(scanSkipSpace)
< 		if op == scanEndObject {
< 			break
< 		}
< 		if op != scanObjectValue {
< 			d.error(errPhase)
< 		}
< 	}
< 	return m
< }
