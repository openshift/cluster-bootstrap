< 	if _, ok := t.fileMap[f.Path]; ok {
< 		panic("Adding already existing file")
< 	}
< 	i := len(*t.files)
< 	*t.files = append(*t.files, f)
< 	t.fileMap[f.Path] = i
< 	return i, &(*t.files)[i]
< }
