< 		out[i], err = time.ParseDuration(d)
< 		if err != nil {
< 			return err
< 		}
< 
< 	}
< 	if !s.changed {
< 		*s.value = out
< 	} else {
< 		*s.value = append(*s.value, out...)
< 	}
