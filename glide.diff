< // NOTE: keep in sync with string above.
< func (e *encodeState) stringBytes(s []byte) (int, error) {
< 	len0 := e.Len()
< 	e.WriteByte('"')
< 	start := 0
< 	for i := 0; i < len(s); {
< 		if b := s[i]; b < utf8.RuneSelf {
< 			if 0x20 <= b && b != '\\' && b != '"' && b != '<' && b != '>' && b != '&' {
< 				i++
< 				continue
