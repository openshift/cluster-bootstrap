< 	case reflect.Uintptr:
< 		printHexPtr(f.fs, uintptr(v.Uint()))
< 
< 	case reflect.UnsafePointer, reflect.Chan, reflect.Func:
< 		printHexPtr(f.fs, v.Pointer())
