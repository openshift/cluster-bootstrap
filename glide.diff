< 	if h, ok := t.(tHelper); ok {
< 		h.Helper()
< 	}
< 
< 	if funcDidPanic, panicValue, _ := didPanic(f); !funcDidPanic {
< 		return Fail(t, fmt.Sprintf("func %#v should panic\n\tPanic value:\t%#v", f, panicValue), msgAndArgs...)
< 	}
< 
< 	return true
< }
