< 	if h, ok := t.(tHelper); ok {
< 		h.Helper()
< 	}
< 
< 	ch := make(chan bool, 1)
< 
< 	timer := time.NewTimer(waitFor)
< 	defer timer.Stop()
< 
< 	ticker := time.NewTicker(tick)
< 	defer ticker.Stop()
< 
< 	for tick := ticker.C; ; {
< 		select {
< 		case <-timer.C:
< 			return Fail(t, "Condition never satisfied", msgAndArgs...)
< 		case <-tick:
< 			tick = nil
< 			go func() { ch <- condition() }()
< 		case v := <-ch:
< 			if v {
< 				return true
< 			}
< 			tick = ticker.C
< 		}
< 	}
< }
