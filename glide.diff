< // Like WriteUnifiedDiff but returns the diff a string.
< func GetUnifiedDiffString(diff UnifiedDiff) (string, error) {
< 	w := &bytes.Buffer{}
< 	err := WriteUnifiedDiff(w, diff)
< 	return string(w.Bytes()), err
< }
