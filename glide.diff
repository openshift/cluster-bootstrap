< 		// Read opening " of string key or closing }.
< 		op := d.scanWhile(scanSkipSpace)
< 		if op == scanEndObject {
< 			// closing } - can only happen on first iteration.
< 			break
< 		}
< 		if op != scanBeginLiteral {
< 			d.error(errPhase)
< 		}
< 
< 		// Read string key.
< 		start := d.off - 1
< 		op = d.scanWhile(scanContinue)
< 		item := d.data[start : d.off-1]
< 		keyEnd := d.off - 1
< 		key, ok := unquote(item)
< 		if !ok {
< 			d.error(errPhase)
< 		}
< 
< 		// Read : before value.
< 		if op == scanSkipSpace {
< 			op = d.scanWhile(scanSkipSpace)
< 		}
< 		if op != scanObjectKey {
< 			d.error(errPhase)
