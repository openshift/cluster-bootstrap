< 		if err != nil {
< 			r.AddOnError(c, err)
< 			return
< 		}
< 		to.Source = &src
< 		tm.AddTranslation(c, path.New("json", "source"))
< 		if gzipped {
< 			to.Compression = util.StrToPtr("gzip")
< 			tm.AddTranslation(c, path.New("json", "compression"))
< 		}
< 	}
