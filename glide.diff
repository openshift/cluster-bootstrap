< // skip scans to the end of what was started.
< func (d *decodeState) skip() {
< 	s, data, i := &d.scan, d.data, d.off
< 	depth := len(s.parseState)
< 	for {
< 		op := s.step(s, data[i])
< 		i++
< 		if len(s.parseState) < depth {
< 			d.off = i
< 			d.opcode = op
< 			return
< 		}
< 	}
